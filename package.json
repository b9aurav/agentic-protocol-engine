{
  "name": "agentic-protocol-engine",
  "version": "1.0.0",
  "description": "AI-driven load testing tool using intelligent LLM agents for realistic user behavior simulation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ape-test": "dist/cli.js",
    "create-ape-test": "dist/create.js"
  },
  "files": [
    "dist/**/*",
    "config/**/*",
    "docker/**/*",
    "services/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && tsc && npm run copy-assets && npm run post-build",
    "post-build": "node scripts/post-build.js",
    "build:watch": "tsc --watch",
    "copy-assets": "node -e \"const fs=require('fs-extra'); ['config','docker','services'].forEach(d=>fs.existsSync(d)&&fs.copySync(d,'dist/'+d))\"",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest --run",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run lint && npm run test:ci && npm run build",
    "prepack": "npm run build",
    "docker:build": "docker build -t agentic-protocol-engine:latest .",
    "docker:publish": "npm run docker:build && docker tag agentic-protocol-engine:latest agentic-protocol-engine:$(npm pkg get version | tr -d '\"')",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major"
  },
  "keywords": [
    "load-testing",
    "ai-agents",
    "llm",
    "performance-testing",
    "docker",
    "mcp"
  ],
  "author": "APE Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "fs-extra": "^11.1.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/inquirer": "^9.0.7",
    "@types/fs-extra": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/b9aurav/agentic-protocol-engine.git"
  },
  "bugs": {
    "url": "https://github.com/b9aurav/agentic-protocol-engine/issues"
  },
  "homepage": "https://github.com/b9aurav/agentic-protocol-engine#readme"
}