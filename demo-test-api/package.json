{
  "name": "demo-test-api",
  "version": "1.0.0",
  "description": "A simple demo API for testing the Agentic Protocol Engine (APE)",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prestart": "npm run build",
    "health": "node health-check.js",
    "test": "node test-endpoints.js",
    "test:endpoints": "node test-endpoints.js",
    "lint": "echo \"No linting configured\" && exit 0"
  },
  "keywords": [
    "api",
    "demo",
    "testing",
    "ape"
  ],
  "author": "APE Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}